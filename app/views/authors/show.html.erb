<div class="card text-center">
  <div class="card-body">
    <h3 class="card-title"><%= params[:user].first_name %> <%= params[:user].last_name %></h3>
    <p class="card-text">Description:<br><%= params[:user].description %></p>
    <p class="card-text">Email:<br><%= params[:user].email %></p>
    <p class="card-text">Age:<br><%= params[:user].age %></p>
    <p class="card-text">Ville:<br><%=link_to params[:user].city.name, city_path %> </p>
  </div>
</div>
