<div class="card text-center">
  <div class="card-body">
    <h5 class="card-title"><%= params[:user].first_name %> <%= params[:user].last_name %></h5>
    <p class="card-text"><%= params[:user].description %></p>
    <p class="card-text"><%= params[:user].email %></p>
    <p class="card-text"><%= params[:user].age %></p>
    <p class="card-text"><%=link_to params[:user].city.name, city_path %> </p>
  </div>
</div>
